#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Data/Junior3rd/HK2/Cross-platform /Final Project/computer_sales_api_microservices/e_commerce/node_modules/.pnpm/ts-node@10.9.2_@types+node@22.15.3_typescript@5.8.3/node_modules/ts-node/dist/node_modules:/Volumes/Data/Junior3rd/HK2/Cross-platform /Final Project/computer_sales_api_microservices/e_commerce/node_modules/.pnpm/ts-node@10.9.2_@types+node@22.15.3_typescript@5.8.3/node_modules/ts-node/node_modules:/Volumes/Data/Junior3rd/HK2/Cross-platform /Final Project/computer_sales_api_microservices/e_commerce/node_modules/.pnpm/ts-node@10.9.2_@types+node@22.15.3_typescript@5.8.3/node_modules:/Volumes/Data/Junior3rd/HK2/Cross-platform /Final Project/computer_sales_api_microservices/e_commerce/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/Data/Junior3rd/HK2/Cross-platform /Final Project/computer_sales_api_microservices/e_commerce/node_modules/.pnpm/ts-node@10.9.2_@types+node@22.15.3_typescript@5.8.3/node_modules/ts-node/dist/node_modules:/Volumes/Data/Junior3rd/HK2/Cross-platform /Final Project/computer_sales_api_microservices/e_commerce/node_modules/.pnpm/ts-node@10.9.2_@types+node@22.15.3_typescript@5.8.3/node_modules/ts-node/node_modules:/Volumes/Data/Junior3rd/HK2/Cross-platform /Final Project/computer_sales_api_microservices/e_commerce/node_modules/.pnpm/ts-node@10.9.2_@types+node@22.15.3_typescript@5.8.3/node_modules:/Volumes/Data/Junior3rd/HK2/Cross-platform /Final Project/computer_sales_api_microservices/e_commerce/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-node/dist/bin-script-deprecated.js" "$@"
else
  exec node  "$basedir/../ts-node/dist/bin-script-deprecated.js" "$@"
fi
